<?xml version="1.0" encoding="utf-8"?><extension type="module" version="2.5.0" client="site" method="upgrade">	<name>ArchPress</name>	<author>UGK</author>	<creationDate>Jul 2017</creationDate>	<copyright>Copyright (C) 2017 UGK.</copyright>	<license>GNU General Public License version 2 or later</license>	<authorEmail>informatyk@klomnice.pl</authorEmail>	<authorUrl>www.klomnice.pl</authorUrl>	<version>3.0.0</version>	<description>Moduł prezentuje galerię złożoną z jpgów i podpina pdf-y. Pliki powinny mieć nazwę: gazeta_1_02.jpg gdzie 1-styczeń 02 - 2002 rok</description>	<files>		<filename module="mod_archpress">mod_archpress.php</filename>		<filename module="mod_archpress">index.html</filename>		<filename module="mod_archpress">tmpl/default.php</filename>		<filename module="mod_archpress">tmpl/index.html</filename>		<filename module="mod_archpress">css/css.css</filename>			<folder>tmpl</folder>	</files>									<config>			<fields name="params">			<fieldset name="BASIC">					<field	name="rok" type="text" default="2006" label="Rok startowy" description="" />				<field	name="width" type="text" default="225" label="Szerokość (px)" description="" />				<field	name="height" type="text" default="318" label="Wysokość (px)" description="" />				<field	name="folder" type="text" default="gazeta_arch" label="Folder z gazetami" description=""  />			</fieldset>			</fields>		</config></extension>